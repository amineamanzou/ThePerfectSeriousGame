#:kivy 1.8
#:import kivy kivy

<StartScreen>:
	FloatLayout:
		DynImage:
			id: map

			source: "Images/start.png"
			pos: root.pos
			size: root.size
			keep_ratio: False
			allow_stretch: True

		DialogWidget:
			id: dialog
			size: (root.width, root.height * 0.2)
			icon: "Images/Characters/profil-test.png"