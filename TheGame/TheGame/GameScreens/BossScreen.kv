#:kivy 1.8
#:import kivy kivy

<BossScreen>:
	FloatLayout:
		DynImage:
			id: map
			source: "Images/Rooms/room_boss.png"
			pos: root.pos
			size: root.size
			keep_ratio: False
			allow_stretch: True
            on_press: root.bossDecision()

		DialogWidget:
			id: dialog
			size: (root.width, root.height * 0.2)
			icon: "Images/Characters/profil-test.png"